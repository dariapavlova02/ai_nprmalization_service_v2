{
  "feature_flags": {
    "use_factory_normalizer": {
      "default": false,
      "description": "Switches to refactored normalization stack with parity tracking",
      "code_touchpoints": ["normalization_factory.py", "normalization_service.py"],
      "primary_tests": ["test_golden_parity.py", "test_ascii_fastpath_equivalence.py"]
    },
    "ascii_fastpath": {
      "default": true,
      "description": "Optimized ASCII tokenization path for English names",
      "code_touchpoints": ["tokenizer_service.py"],
      "primary_tests": ["test_ascii_fastpath_equivalence.py"]
    },
    "fix_initials_double_dot": {
      "default": false,
      "description": "Fix double dots in initials (A..B -> A.B.)",
      "code_touchpoints": ["tokenizer_service.py"],
      "primary_tests": ["test_initials_ru.py"]
    },
    "preserve_hyphenated_case": {
      "default": true,
      "description": "Preserve case in hyphenated names",
      "code_touchpoints": ["tokenizer_service.py"],
      "primary_tests": ["test_initials_and_hyphen.py"]
    },
    "strict_stopwords": {
      "default": false,
      "description": "Strict stopwords filtering (currently no-op)",
      "code_touchpoints": ["role_tagger_service.py"],
      "primary_tests": ["test_role_tagging_extended.py"]
    },
    "enable_ac_tier0": {
      "default": true,
      "description": "Enable tier 0 AC matching",
      "code_touchpoints": ["unified_pattern_service.py", "elasticsearch_adapters.py"],
      "primary_tests": ["test_search_integration.py"]
    },
    "enable_vector_fallback": {
      "default": true,
      "description": "Enable vector search fallback",
      "code_touchpoints": ["hybrid_search_service.py", "enhanced_vector_index_service.py"],
      "primary_tests": ["test_vector_fallback.py"]
    },
    "enable_rapidfuzz_rerank": {
      "default": true,
      "description": "Enable RapidFuzz reranking",
      "code_touchpoints": ["hybrid_search_service.py"],
      "primary_tests": ["test_reranking.py"]
    },
    "enable_dob_id_anchors": {
      "default": true,
      "description": "Enable DoB/ID anchors in signals",
      "code_touchpoints": ["signals_service.py"],
      "primary_tests": ["test_signals_service.py"]
    },
    "enforce_nominative": {
      "default": true,
      "description": "Enforce nominative case for Russian/Ukrainian",
      "code_touchpoints": ["morphology_adapter.py"],
      "primary_tests": ["test_morph_and_diminutives.py"]
    },
    "preserve_feminine_surnames": {
      "default": true,
      "description": "Preserve feminine surname forms",
      "code_touchpoints": ["morphology_adapter.py"],
      "primary_tests": ["test_morph_and_diminutives.py"]
    },
    "enable_decision_engine": {
      "default": true,
      "description": "Enable decision engine processing",
      "code_touchpoints": ["decision_engine.py"],
      "primary_tests": ["test_decision_engine.py"]
    },
    "ENABLE_SMART_FILTER": {
      "default": true,
      "description": "Enable smart filter for low-risk inputs",
      "code_touchpoints": ["orchestrator_factory.py"],
      "primary_tests": ["test_smart_filter.py"]
    },
    "ENABLE_EMBEDDINGS": {
      "default": true,
      "description": "Enable embeddings processing",
      "code_touchpoints": ["orchestrator_factory.py"],
      "primary_tests": ["test_embeddings.py"]
    }
  },
  "environment_flags": {
    "AI_DECISION__THR_AUTO_HIT": {
      "default": 0.8,
      "description": "Auto-hit threshold for decision engine"
    },
    "AI_DECISION__THR_REVIEW": {
      "default": 0.6,
      "description": "Review threshold for decision engine"
    },
    "AI_DECISION__THR_REJECT": {
      "default": 0.3,
      "description": "Reject threshold for decision engine"
    }
  }
}
